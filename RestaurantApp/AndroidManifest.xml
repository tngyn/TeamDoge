<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamdoge.restaurantapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.teamdoge.login.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.teamdoge.restaurantapp.Main_ExpandableList"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.teamdoge.login.SignUpActivity"
            android:label="@string/title_activity_signup"
            android:parentActivityName="com.teamcom.teamdoge.login.LoginActivity"
            android:windowSoftInputMode="adjustResize|stateVisible" >
        </activity>
        <activity
            android:name="com.teamdoge.restaurantapp.MainActivity"
            android:windowSoftInputMode="adjustResize|stateVisible" >
        </activity>
        <activity
            android:name="com.teamdoge.restaurantappW.adding_inventory_item_page"
            android:label="@string/inv_item_page" >
        </activity>
